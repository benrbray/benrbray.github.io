---
import type { CollectionEntry } from 'astro:content';
import SeriesSummary from '../content/SeriesSummary.astro';

interface Props {
  series: CollectionEntry<'series'>[];
}

const { series } = Astro.props;
---

<div class="series-list">
{series.map(s => (
  <SeriesSummary series={s} />
))}
</div>